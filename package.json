{
  "name": "kozette-server",
  "description": "Server side part of Kozette",
  "version": "0.0.0",
  "homepage": "",
  "main": "src/",
  "keywords": [
    "feathers"
  ],
  "license": "MIT",
  "repository": {},
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 0.12.0"
  },
  "scripts": {
    "start": "env NODE_ENV=development node src/",
    "mocha": "mocha test/ --recursive",
    "jshint": "jshint src/. test/. --config",
    "test": "npm run jshint && env NODE_ENV=test npm run mocha",
    "travis": "npm run jshint && env NODE_ENV=travis npm run mocha",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "compression": "^1.6.1",
    "cors": "^2.7.1",
    "feathers": "^2.0.1",
    "feathers-authentication": "^0.7.5",
    "feathers-configuration": "^0.2.3",
    "feathers-errors": "^2.1.0",
    "feathers-hooks": "^1.5.2",
    "feathers-mongoose": "^3.3.7",
    "feathers-nedb": "^2.2.3",
    "feathers-socketio": "^1.4.0",
    "moment": "^2.13.0",
    "mongoose": "^4.4.14",
    "nedb": "^1.8.0",
    "passport": "^0.3.2",
    "serve-favicon": "^2.3.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.11.12",
    "istanbul": "^0.4.4",
    "jshint": "^2.9.2",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
